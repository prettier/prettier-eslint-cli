{
  "name": "prettier-eslint-cli",
  "version": "0.0.0-development",
  "description": "CLI for prettier-eslint",
  "main": "dist/no-main.js",
  "engines": {
    "node": ">=12.22"
  },
  "bin": {
    "prettier-eslint": "dist/index.js"
  },
  "scripts": {
    "start": "nps",
    "test": "nps test",
    "precommit": "lint-staged && opt --in pre-commit --exec \"npm start validate\"",
    "prepare": "husky install && patch-package"
  },
  "files": [
    "dist"
  ],
  "keywords": [],
  "author": "Kent C. Dodds <kent@doddsfamily.us> (http://kentcdodds.com/)",
  "maintainers": [
    "Simon Fridlund <simon@fridlund.email>"
  ],
  "license": "MIT",
  "dependencies": {
    "arrify": "^2.0.1",
    "boolify": "^1.0.1",
    "camelcase-keys": "^7.0.2",
    "chalk": "^4.1.2",
    "common-tags": "^1.8.2",
    "core-js": "^3.24.1",
    "eslint": "^8.21.0",
    "find-up": "^5.0.0",
    "get-stdin": "^8.0.0",
    "glob": "^7.2.3",
    "ignore": "^5.2.0",
    "indent-string": "^4.0.0",
    "lodash.memoize": "^4.1.2",
    "loglevel-colored-level-prefix": "^1.0.0",
    "messageformat": "^2.3.0",
    "prettier-eslint": "^15.0.1",
    "rxjs": "^7.5.6",
    "yargs": "^13.1.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.18.10",
    "@babel/core": "^7.18.10",
    "@babel/node": "^7.18.10",
    "@babel/preset-env": "^7.18.10",
    "@babel/register": "^7.18.9",
    "acorn": "^8.8.0",
    "ajv": "^8.11.0",
    "all-contributors-cli": "^6.20.0",
    "babel-jest": "^28.1.3",
    "eslint-config-kentcdodds": "^20.3.1",
    "husky": "^8.0.1",
    "jest": "^28.1.3",
    "jest-cli": "^28.1.3",
    "lint-staged": "^13.0.3",
    "nps": "^5.10.0",
    "nps-utils": "^1.7.0",
    "opt-cli": "^1.6.0",
    "patch-package": "^6.4.7",
    "pify": "^5.0.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^19.0.3",
    "spawn-command": "0.0.2-1",
    "strip-indent": "^3.0.0",
    "yargs-parser": "^21.1.1"
  },
  "lint-staged": {
    "*.+(js|json|md)": [
      "node ./dist/index --write",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/prettier/prettier-eslint-cli.git"
  },
  "bugs": {
    "url": "https://github.com/prettier/prettier-eslint-cli/issues"
  },
  "homepage": "https://github.com/prettier/prettier-eslint-cli#readme"
}
